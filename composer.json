{
    "name": "sylius/sylius-standard",
    "type": "project",
    "description": "Starting point for projects powered by Sylius eCommerce.",
    "keywords": [
        "symfony",
        "sylius",
        "distribution",
        "ecommerce",
        "cart",
        "shopping"
    ],
    "homepage": "https://sylius.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski"
        },
        {
            "name": "Sylius project"
        },
        {
            "name": "Community contributions"
        }
    ],
    "require": {
        "php": "^8.2",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "guzzlehttp/guzzle": "*",
        "knplabs/knp-menu-bundle": "^3.5",
        "liip/imagine-bundle": "^2.14",
        "mailgun/mailgun-php": "*",
        "sylius/cms-plugin": "*",
        "sylius/sylius": "~2.0.0",
        "sylius/twig-hooks": "^0.6.1",
        "symfony/dotenv": "^6.4 || ^7.1",
        "symfony/flex": "^2.4",
        "symfony/mailgun-mailer": "^7.1",
        "symfony/runtime": "^6.4 || ^7.1",
        "vich/uploader-bundle": "*"
    },
    "require-dev": {
        "behat/behat": "^3.14",
        "behat/mink-selenium2-driver": "^1.7",
        "dbrekelmans/bdi": "^1.3",
        "deployer/deployer": "^7.5",
        "dmore/behat-chrome-extension": "^1.4",
        "dmore/chrome-mink-driver": "^2.9",
        "doctrine/doctrine-fixtures-bundle": "*",
        "friends-of-behat/mink": "^1.11",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "friends-of-behat/mink-debug-extension": "^2.1",
        "friends-of-behat/mink-extension": "^2.7",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.1",
        "friends-of-behat/symfony-extension": "^2.6",
        "friends-of-behat/variadic-extension": "^1.6",
        "lchrusciel/api-test-case": "^5.3",
        "nyholm/psr7": "^1.8",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "phpunit/phpunit": "^10.5",
        "robertfausk/behat-panther-extension": "^1.1",
        "sylius-labs/coding-standard": "^4.4",
        "sylius-labs/suite-tags-extension": "~0.2",
        "sylius/fixtures-bundle": "^1.9",
        "sylius/sylius-rector": "^3.0",
        "symfony/browser-kit": "^6.4 || ^7.1",
        "symfony/debug-bundle": "^6.4 || ^7.1",
        "symfony/http-client": "^6.4 || ^7.1",
        "symfony/intl": "^6.4 || ^7.1",
        "symfony/web-profiler-bundle": "^6.4 || ^7.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "php-http/discovery": false,
            "phpstan/extension-installer": false,
            "rector/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "symfony/thanks": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.0-dev"
        },
        "symfony": {
            "allow-contrib": "true",
            "require": "^7.1",
            "endpoint": ["https://api.github.com/repos/Sylius/SyliusRecipes/contents/index.json?ref=flex/main", "flex://defaults"]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "classmap": [
            "src/Kernel.php"
        ]
    },
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "/opt/cpanel/ea-php82/root/usr/bin/php -dmemory_limit=-1 bin/console cache:clear --env=prod",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "/opt/cpanel/ea-php82/root/usr/bin/php -dmemory_limit=-1 bin/console cache:clear --env=prod",
            "@auto-scripts"
        ],
        "post-create-project-cmd": [
            "@php bin/console sylius:inform-about-gus --ansi",
            "@php bin/console sylius:show-available-plugins --ansi"
        ],
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "reload-dev-fixtures": [
            "bin/console d:d:d -f || true",
            "bin/console d:d:c",
            "bin/console doctrine:migration:migrate -n",
            "bin/console sylius:fixtures:load hangout -n -v"
        ]
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    }
}
